{
	"name": "sveltycms.com",
	"description": "SveltyCMS - A powerful Headless CMS with sveltekit power",
	"version": "0.0.1",
	"scripts": {
		"---- DEVELOP ----------------------------------------------------------": "",
		"dev": "vite dev --open",
		"devv": "LAUNCH_EDITOR='code' vite --open",
		"---- BUILD ----------------------------------------------------------": "",
		"build": "paraglide-js compile --project ./project.inlang && vite build",
		"---- CHECK & TEST ----------------------------------------------------------": "",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json  --threshold error",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"test": "paraglide-js compile && playwright test",
		"test:e2e": "pnpm dev & pnpx wait-on http://localhost:5173 && playwright test",
		"---- LINT & FORMAT ------------------------------------------------------------": "",
		"lint": "prettier --check . && eslint .",
		"format": "prettier --write .",
		"---- OTHER ----------------------------------------------------------": "",
		"preview": "vite preview"
	},
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "https://github.com/Rar9/SveltyCMS"
	},
	"bugs": {
		"url": "https://github.com/Rar9/SveltyCMS/issues"
	},
	"keywords": [
		"cms",
		"content-management-system",
		"DXP Digital",
		"Developer Experience Platform",
		"inlang",
		"i18n",
		"internationalization",
		"paraglide-js",
		"headless",
		"headless-cms",
		"graphql",
		"skeleton",
		"svelte",
		"svelte-cms",
		"sveltekit",
		"sveltekit_cms",
		"sveltycms",
		"oauth",
		"tailwind",
		"tailwindcss",
		"rich-text-editor",
		"lexical-editor",
		"mongodb"
	],
	"devDependencies": {
		"@iconify/svelte": "^4.0.2",
		"@inlang/paraglide-js": "^1.11.2",
		"@inlang/paraglide-sveltekit": "0.8.1",
		"@playwright/test": "^1.45.2",
		"@skeletonlabs/skeleton": "2.10.0",
		"@skeletonlabs/tw-plugin": "0.4.0",
		"@sveltejs/adapter-node": "^5.2.0",
		"@sveltejs/enhanced-img": "^0.2.1",
		"@sveltejs/kit": "^2.5.18",
		"@sveltejs/vite-plugin-svelte": "^3.1.1",
		"@tailwindcss/forms": "0.5.7",
		"@tailwindcss/typography": "0.5.13",
		"@types/eslint": "^8.56.10",
		"@types/node": "20.12.12",
		"@typescript-eslint/eslint-plugin": "^7.16.1",
		"@typescript-eslint/parser": "^7.16.1",
		"autoprefixer": "10.4.19",
		"eslint": "^8.57.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-svelte": "^2.42.0",
		"postcss": "8.4.38",
		"prettier": "^3.3.3",
		"prettier-plugin-svelte": "^3.2.5",
		"svelte": "^4.2.18",
		"svelte-check": "^3.8.4",
		"tailwindcss": "3.4.3",
		"tslib": "^2.6.3",
		"typescript": "^5.5.3",
		"vite": "^5.3.4",
		"vite-plugin-tailwind-purgecss": "0.3.3",
